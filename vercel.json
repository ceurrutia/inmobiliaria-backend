{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "routes/**/*.js", "use": "@vercel/node" },
    { "src": "controllers/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/properties", "dest": "/routes/propertyRoutes.js" },
    { "src": "/api/users", "dest": "/routes/userRoutes.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
  